package variaveis.definirVariaveis;

public class DefinirNormalmente {

	public static void main(String[] args) {
		
		int inteiro = 42;
		byte baite = -128;
		short pequeno = 3276;
		long longo = 98448870363655355l;
		float fluante = 4.44f;
		double quebrado = 3.14;
		String texto = "Meu texto";
		
		
	}
	
}
